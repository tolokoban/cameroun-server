{
    "name": "cameroun-server",
    "author": "tolokoban",
    "description": "Centralisation des données provenant des différents hôpitaux utilisant un logiciel de suivi patient.",
    "repository": {
        "type": "git",
        "url": "https://github.com/tolokoban/cameroun-server.git"
    },
    "homepage": "https://github.com/tolokoban/cameroun-server",
    "license": "GPL-3.0",
    "scripts": {
        "test": "node node_modules/karma/bin/karma start karma.conf.js",
        "test:dbg": "node --debug-brk node_modules/karma/bin/karma start karma.conf.js",
        "debug": "node submodules/tfw/bin/tfw.js build debug test php",
        "watch": "node submodules/tfw/bin/tfw.js build debug test php watch",
        "clean": "node submodules/tfw/bin/tfw.js clean",
        "release": "node submodules/tfw/bin/tfw.js build version php clean"
    },
    "bugs": {
        "url": "https://github.com/tolokoban/cameroun-server/issues"
    },
    "tfw": {
        "resources": [],
        "modules": [],
        "compile": {
            "type": "firefoxos",
            "files": [
                "\\.html$"
            ]
        },
        "output": "../../www/cameroun-server"
    },
    "version": "0.0.1",
    "reservedModules": [],
    "dependencies": {
        "jasmine-core": "^2.4",
        "karma": "^0.13",
        "karma-chrome-launcher": "^0.2",
        "karma-firefox-launcher": "^0.1",
        "karma-jasmine": "^0.3.6"
    },
    "devDependencies": {}
}
